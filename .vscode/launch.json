{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (Global.Api)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildApi",
            "program": "${workspaceRoot}/src/Rwby.Global/Rwby.Global.Api/bin/Debug/netcoreapp1.1/Rwby.Global.Api.dll",
            "args": [],
            "cwd": "${workspaceRoot}\\src\\Rwby.Global\\Rwby.Global.Api",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": true,
                "args": "http://localhost:50707/",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start http://localhost:50707/"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Production"
            },
            "sourceFileMap": {
                "/Views": "${workspaceRoot}/Views"
            }
        },
        {
            "name": "Launch (Global.Web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildWeb",
            "program": "${workspaceRoot}/src/Rwby.Global/Rwby.Global.Web/bin/Debug/netcoreapp1.1/Rwby.Global.Web.dll",
            "args": [],
            "cwd": "${workspaceRoot}\\src\\Rwby.Global\\Rwby.Global.Web",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "http://localhost:50274/",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start http://localhost:50274/"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Production"
            },
            "sourceFileMap": {
                "/Views": "${workspaceRoot}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}